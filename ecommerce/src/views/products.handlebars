<div>
    {{#if isAdmin}}
         <ul>
            {{#each products}}
            <hr>
            <li>{{title}}</li>
            <li>{{description}}</li>
            <li>{{code}}</li>
            <li>{{price}}</li>
            <li>{{stock}}</li>
            <li>{{category}}</li>
            <li><img src="{{thumbnail}}" alt="" height="100px"></li>
            {{/each}}
         </ul>
    {{else}}
        <div>Producto no autorizado</div>
    {{/if}}
</div>